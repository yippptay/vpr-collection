<style>
  .header_1_wrapper{
    background-color:{{section.settings.color_menu_bg }};
  }
  .header_1_wrapper .main-header{
    background-color:{{section.settings.main_bgheader }};
    border-bottom: 1px solid {{ settings.color_borders }};
  }
  .header_1 .site-header__cart-count .cart-products-count,.header_1.site-header .compare-count{
    color:{{ section.settings.count_color }};
    background:{{ section.settings.countbgcolor }};
  }
  .tthorizontal_menu #tt-megamenu1 .tt-mega_menu1 li.tt_menu_item > a, .tthorizontal_menu #tt-megamenu1 .tt-mega_menu1 li.tt_menu_item span.categories{
    color:{{ section.settings.hrmenu_txt_clr }};
  }
  .header_1 .right-link-icon .myaccount .dropdown-toggle,.header_1 .site-header__cart-count .cart-products-detail,.header_1 .wrapper-top-cart>.site-header_cart_link svg,.header_1 .wishlist-icon-div a svg,.header_1 .compare-icon-div a svg,.header_1.site-header .right-link-icon .contact .contact-text .contact_no,.header_1.site-header .right-link-icon .contact{
    color:{{section.settings.Header_link_color }};
  }
  .header_1 .right-link-icon .myaccount .dropdown-toggle:hover,.header_1.site-header .right-link-icon .contact:hover svg,.header_1.site-header .right-link-icon .contact:hover .contact-text .contact_no,.header_1 .site-header_cart_link:hover .site-header__cart-count .cart-products-detail,.header_1 .wrapper-top-cart>.site-header_cart_link:hover svg,.header_1 .wrapper-top-cart>.site-header_cart_link.active svg,.header_1 .right-link-icon .myaccount .dropdown-toggle.open,.header_1 .wishlist-icon-div:hover a>i,.header_1 .compare-icon-div:hover a>i{
    color:{{section.settings.Header_link_hover_color }};
  }
  .header_1 .top-header,.top_header_1 .header_language .disclosure .language_wrapper,.header_1 .topright-header a,
  .header_1.site-header span.language,.header_1.site-header span.currency,
  .top_header_1 a.currency_wrapper.dropdown-toggle{color:{{section.settings.top_header_text_color }};}

  .top_header_1 .header_language .disclosure .language_wrapper:hover,.header_1 .topright-header a:hover,.top_header_1 .header_language .disclosure .language_wrapper.active, 
  .top_header_1 .currency_wrapper.dropdown-toggle:hover,.top_header_1 .currency_wrapper.dropdown-toggle.active{
    color:{{section.settings.top_header_text_hover_color }};
  }
  .header_1 .top_header_1{ background-color:{{section.settings.top_bg_color }};}
  .header_1 .bottom_header_1{ background-color:{{section.settings.bottom_bg_color }}; border-bottom: 1px solid {{ settings.color_borders }};}  
  #tt-megamenu span.toggle_menu{ background:{{section.settings.tt_title_bgclr }}; color:{{section.settings.tt_title_clr }};}
  .tt-mega-menu #tt-megamenu .tt-mega_menu{ background:{{section.settings.menu_bgclr }};}
  #tt-megamenu .tt-mega_menu .tt_menus_ul>li:hover>a{color:{{section.settings.menu_txt_hvr_clr }};}
  #tt-megamenu .tt-mega_menu .tt_menus_ul>li>a{color:{{section.settings.menu_txt_clr }};}  
  .tt-mega-menu #tt-megamenu .tt-mega_menu{border: 2px solid {{section.settings.bottom_bg_color }}; border-top: transparent}
</style>
{% capture navSize %}250px{% endcapture %}
{% capture navHeaderSize %}{{ section.settings.menu_mobile }}px{% endcapture %}
{% capture logoMaxHeight %}{{ section.settings.menu_mobile | minus: 30 }}px{% endcapture %}
{% capture borderWidth %}{{ settings.border_width }}px{% endcapture %}
{% capture colorBorder %}{{ settings.color_borders }}{% endcapture %}
{% assign navMenu = linklists[section.settings.nav_menu] %}
{%- assign sticky_class = '' -%}
{% if section.settings.sticky_header %}{%- assign sticky_class = 'sticky_header' -%}{% endif %}
<header class="site-header page-element header_1" data-section-id="{{ section.id }}" data-section-type="header-section">
  <div class="header_1_wrapper">
    <div class="top_header_1">
      <div class="container">  
        <div class="top-header-content">
          <div class="top-header">
            <a class="track-order" href="{% if section.settings.track_link != blank %}{{ section.settings.track_link }}{% else %}#{% endif %}" title="track-order">
              <i class="mdi mdi-tractor"></i>
              <div class="destination">{{ section.settings.destination }}</div>
            </a>
            {% if section.settings.display_language %}             
            <div class="header_language">
              <span class="language">{{ 'general.language.label' | t }}</span>
              <ul class="tbl-list">              
                <li class="language dropdown-parent uppercase language-block"> 
                  {% render 'language-switcher' %}
                </li>
              </ul>
            </div>
            {% endif %}
            {% if section.settings.display_currency %}             
            <div class="header_currency">
              <span class="currency">{{ 'general.currency.label' | t }}</span>
              <ul class="tbl-list">              
                <li class="currency dropdown-parent uppercase currency-block">
                  {% if section.settings.display_currency %}    
                  {% render 'currencies-switcher' %}
                  {% endif %}
                </li>
              </ul>
            </div>
            {% endif %}

          </div>
          <div class="topright-header">
            {% if section.settings.page1_title != blank %}
            <a href="{% if section.settings.page1_url != blank %}{{ section.settings.page1_url }}{% else %}#{% endif %}" class="page1">{{ section.settings.page1_title }}</a>
            {% if section.settings.page2_title != blank %}
            {% endif %} 
            <a href="{% if section.settings.page2_url != blank %}{{ section.settings.page2_url }}{% else %}#{% endif %}" class="page2">{{ section.settings.page2_title }}</a>
            {% endif %} 
          </div>
          {% if section.settings.flash_title != blank %}
          <div class="flash-sale">
            <a class="flash-content" href="{% if section.settings.flash_content_url != blank %}{{ section.settings.flash_content_url }}{% else %}#{% endif %}">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-zap">
                <polygon points="13 2 3 14 12 14 11 22 21 10 12 10 13 2"></polygon></svg>
              <div class="flash-title">{{ section.settings.flash_title }}</div>
            </a>
            <div class="list_product_menu_content owl-carousel">
              {%- assign collection = collections[section.settings.tt_sub_menu_pproduts] -%}            
              {% if collection != blank %}
              {% for product in collection.products limit: menu_product_limit %}
              <div class="item-row product-layouts col-xs-12 grid-view-item{% unless product.available %} product-price--sold-out grid-view-item--sold-out{% endunless %}" data-ajax-enabled="{{ settings.use_ajax_cart }}">  
                <div class="product-wrapper">    
                  <div class="thumbs product-thumb">
                    <div class="grid-view-item {{ pmenu_items_mob }} {{ pmenu_items_tab }} {{ pmenu_items_des }} {{ settings.product_img_hover }}">
                      <a class="grid-view-item__link grid-view-item__links" href="{{ product.url | within: collection }}">
                        <img loading="lazy" class="grid-view-item__image featured-image image_thumb" height="230" width="209" src="{{ product.featured_media | img_url: settings.product_image_width_height }}" alt="{{ product.featured_image.alt }}">
                        {% if product.images.size > 1 %}
                        {% if product.images.last %} 
                        <div class="second-productimg">
                          <img loading="lazy" class="image_thumb_swap" height="230" width="209" src="{{ product.images.last | product_img_url: settings.product_image_width_height }}" alt="{{ product.images.last.alt | escape }}" />
                        </div>
                        {% endif %} 
                        {% endif %}
                      </a>
                      {% if product.available %} 
                      {% else %}
                      <div class="sold-out-tag text-center">
                        <span class="tag">{{ 'products.product.sold_out' | t }}</span>
                      </div>
                      {% endif %}
                      <div class="product_info">
                        <div class="h4 grid-view-item__title">
                          <a class="grid-view-item__link grid-link__title" href="{{ product.url | within: collection }}">
                            {{ product.title | strip_html | truncate: 35 }}
                          </a>
                        </div>
                        <div class="grid-view-item__meta">
                          {% render 'product-price-listing', product: product %}
                        </div> 
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              {% endfor %}
              {% endif %}
            </div>
          </div>
          {% endif %}
        </div>
      </div>
    </div>
    <div id="header-sticky" class="main-header {{ sticky_class }}">
      <div class="container">
      <div class="center-header">
        <div class="header_logo_wrap">
          <div class="h1 header-logo" itemscope itemtype="http://schema.org/Organization">
            {% if section.settings.logo != blank %}
            <a href="{{ routes.root_url }}" itemprop="url">
              <img loading="lazy" src="{{ section.settings.logo | img_url: 'master' }}" alt="{{ shop.name }}" title="{{ shop.name }}" itemprop="logo">
            </a>
            {% else %}
            <a href="{{ routes.root_url }}" itemprop="url">{{ shop.name }}</a>
            {% endif %}
          </div>
        </div>
        {% if section.settings.display_search %}  
        <div class="site-header__search icon-btn col-sm-6">
          <div class="search_wrapper">
            {% render 'search-bar' %} 
          </div>
        </div> 
        {% endif %}
        <div class="right-link-icon">
          <div class="contact">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-headphones"><path d="M3 18v-6a9 9 0 0 1 18 0v6">
              </path><path d="M21 19a2 2 0 0 1-2 2h-1a2 2 0 0 1-2-2v-3a2 2 0 0 1 2-2h3zM3 19a2 2 0 0 0 2 2h1a2 2 0 0 0 2-2v-3a2 2 0 0 0-2-2H3z"></path></svg>
            <div class="contact-text">
              <a class="contact_no" href="tel:{{ section.settings.contact_no }}">{{ section.settings.contact_no }}</a>
              <span class="contact_detail">{{ section.settings.contact_detail }}</span>
            </div>
          </div>
          {% if section.settings.display_myaccount %}
          <div class="myaccount icon-btn">
            <span class="dropdown-toggle" title="My Account" data-toggle="dropdown">              
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-user">
                <path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path><circle cx="12" cy="7" r="4"></circle></svg>
              <div class="account-text">
                <span class="account-title">{{ 'customer.account.title' | t }}</span>
                {% if shop.customer_accounts_enabled %}
                <div class="account-detail">
                  {% if customer %}
                  <span>{{ 'customer.account.hello' | t }} , <span class="username">{{ customer.first_name }} / </span></span>
                  <span class="logout-link">{{ 'layout.customer.log_out' | t | customer_logout_link }}</span>
                  {% else %}
                  <span >{{ 'layout.customer.log_in' | t }} / </span>
                  <span >{{ 'layout.customer.create_account' | t }}</span>         
                  {% endif %} 
                </div>
                {% endif %} 
              </div>
              <i class="mdi mdi-chevron-down" aria-hidden="true"></i>
              <i class="mdi mdi-chevron-up" aria-hidden="true"></i>
            </span>
            <div class="customer_account">  
              <ul>
                {% if shop.customer_accounts_enabled %}
                {% if customer %}
                <li>
                  <i class="mdi mdi-account-plus-outline"></i>
                  <a href="/account" title="{{ 'customer.account.hello' | t }}">{{ 'customer.account.hello' | t }} , <span class="username">{{ customer.first_name }}</span></a>
                </li>
                <li class="logout-link">
                  <i class="mdi mdi-logout"></i>
                  {{ 'layout.customer.log_out' | t | customer_logout_link }}
                </li>
                {% else %}
                <li>
                  <i class="mdi mdi-login"></i>
                  <a href="/account/login" title="{{ 'layout.customer.log_in' | t }}">{{ 'layout.customer.log_in' | t }}</a>
                </li>
                <li>
                  <i class="mdi mdi-account-plus-outline"></i>
                  <a href="/account/register" title="{{ 'layout.customer.create_account' | t }}">{{ 'layout.customer.create_account' | t }}</a>
                </li>          
                {% endif %}  
                {% endif %} 
              </ul>
            </div>
          </div>
          {% endif %}
          {% if settings.enable_add_cart %}  
          <div class="site-header__cart icon-btn">
            <div class="wrapper-top-cart">                 
              <a href="/cart" id="cart-products-count" class="site-header_cart_link">                    
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-shopping-bag">
                  <path d="M6 2L3 6v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2V6l-3-4z"></path><line x1="3" y1="6" x2="21" y2="6"></line><path d="M16 10a4 4 0 0 1-8 0"></path></svg>
                {% if cart.item_count > 0 %}
                <div id="CartCount_sticky" class="site-header__cart-count" title="Cart" >
                  <span class="cart-products-count" aria-hidden="true">{{ cart.item_count }}</span><span class="cart-products-detail">{{ 'general.header.cart_title' | t }}</span>  
                  <span class="price">{{cart.total_price | money}}</span>
                </div>
                {% else %}
                <div id="CartCount_sticky" class="site-header__cart-count">
                  <span class="cart-products-count" aria-hidden="true">0</span><span class="cart-products-detail">{{ 'general.header.cart_title' | t }}</span>   
                  <span class="price">{{cart.total_price | money}}</span>
                </div>
                {% endif %}
              </a>
            </div>
          </div>
          <div class="fixed-cart-wrap">
            {% render 'mini-cart' %} 
          </div>
          {% endif %}
        </div>
      </div>
    </div>
    </div>
    <div class="bottom_header_1">
      <div class="container">
        <div class="ttresponsive_menu">
          {% if section.blocks.size > 0 %}
          <div id="shopify-section-TT-megamenu" class="shopify-section tt-mega-menu col-sm-3">
            <div id="tt-megamenu" class="tt-megamenu">
              <span class="toggle_menu current-close" title="menu-icon">
                <i class="mdi mdi-menu"></i>
                <span class="toggle_text">{{ section.settings.tt-title }}</span>                
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 65.000000 65.000000" preserveAspectRatio="xMidYMid meet">
                  <g transform="translate(0.000000,65.000000) scale(0.100000,-0.100000)" fill="#000000" stroke="none">
                    <path d="M510 575 l0 -65 65 0 65 0 0 65 0 65 -65 0 -65 0 0 -65z"/><path d="M260 325 l0 -65 65 0 65 0 0 65 0 65 -65 0 -65 0 0 -65z"/><path d="M510 325 l0 -65 65 0 65 0 0 65 0 65 -65 0 -65 0 0 -65z"/>
                    <path d="M10 75 l0 -65 65 0 65 0 0 65 0 65 -65 0 -65 0 0 -65z"/><path d="M260 75 l0 -65 65 0 65 0 0 65 0 65 -65 0 -65 0 0 -65z"/><path d="M510 75 l0 -65 65 0 65 0 0 65 0 65 -65 0 -65 0 0 -65z"/>
                  </g>
                </svg>
              </span> 
              <div class="tt-mega_menu">
                <ul class="tt_menus_ul">
                  {%- assign i = 0 -%}
                  {%- assign ik = 0 -%}
                  {% for block in section.blocks %}
                  {%- assign i = i | plus: 1 -%}
                  {% if block.type == 'tt_mm_menu' %}
                  {% if i == 1 %}
                  <li class="tt_menu_item{% if block.settings.full_menu == true %} full-menu{% endif %}{% if block.settings.tt_mn_show_sub == true %} tt_mm_hassub{% endif %}{% if block.settings.tt_mn_show_sub == false %} single{% endif %}{% if block.settings.tt_mn_class != blank %} {{ block.settings.tt_mn_class }}{% endif %}">
                    <a href="{{ block.settings.button_link }}">
                      {% if block.settings.tt_sub_menu_icon != blank %}
                      <span id="menuicon-{{ block.id }}" class="icon">
                        <img loading="lazy" src="{{ block.settings.tt_sub_menu_icon | img_url: 'master'}}" alt="menu-icon"/>
                      </span>
                      {% endif %}
                      <span class="tt_menu_item_heading_title">{{ block.settings.title }}
                        {% if block.settings.tt_mn_sub_heading != blank %}
                        <span class="tt_mn_sub_heading" style="{% if block.settings.tt_mn_subheading_bg != blank %}background-color:{{ block.settings.tt_mn_subheading_bg }};{% endif %}{% if block.settings.tt_mn_subheading_color != blank %}color:{{ block.settings.tt_mn_subheading_color }};{% endif %}">{{ block.settings.tt_mn_sub_heading }}</span>{% endif %}
                      </span>
                    </a>                    
                    {% if block.settings.tt_mn_show_sub == true %}  
                    <span class="mobile-nav__sublist-trigger">
                      <span class="mobile-nav__sublist-expand">
                        <i class="mdi mdi-chevron-down" aria-hidden="true"></i>
                      </span>
                      <span class="mobile-nav__sublist-contract">
                        <i class="mdi mdi-chevron-up" aria-hidden="true"></i>
                      </span>
                    </span>{% endif %}
                    <div class="tt_sub_menu_wrap {% if block.settings.tt_mn_show_sub == false %}disable{% endif %}" {% if block.settings.tt_mn_sub_contentwidth != blank %}style="width:{{ block.settings.tt_mn_sub_contentwidth }}"{% endif %} >
                      {% else %}
                    </div>
                  </li>
                  <li id="menu--{{ block.id }}" class="{% if block.settings.highlight_menu == true %}highlight-menu {% endif %}tt_menu_item{% if block.settings.full_menu == true %} full-menu{% endif %}{% if block.settings.tt_mn_show_sub == true %} tt_mm_hassub{% endif %}{% if block.settings.tt_mn_show_sub == false %} single{% endif %} {% if block.settings.tt_mn_class != blank %} {{ block.settings.tt_mn_class }}{% endif %}">
                    <a href="{{ block.settings.button_link }}">
                      {% if block.settings.tt_sub_menu_icon != blank %}
                      <span id="menuicon-{{ block.id }}" class="icon">
                        <img loading="lazy" src="{{ block.settings.tt_sub_menu_icon | img_url: 'master'}}" alt="menu-icon"/>
                      </span>
                      {% endif %}
                      <span class="tt_menu_item_heading_title">{{ block.settings.title }}
                        {% if block.settings.tt_mn_sub_heading != blank %}
                        <span class="tt_mn_sub_heading" style="{% if block.settings.tt_mn_subheading_bg != blank %}background-color:{{ block.settings.tt_mn_subheading_bg }};{% endif %}{% if block.settings.tt_mn_subheading_color != blank %}color:{{ block.settings.tt_mn_subheading_color }};{% endif %}">{{ block.settings.tt_mn_sub_heading }}</span>{% endif %}
                      </span>
                    </a>                    
                    {% if block.settings.tt_mn_show_sub == true %}
                    <span class="mobile-nav__sublist-trigger">
                      <span class="mobile-nav__sublist-expand">
                        <i class="mdi mdi-chevron-down" aria-hidden="true"></i>
                      </span>
                      <span class="mobile-nav__sublist-contract">
                        <i class="mdi mdi-chevron-up" aria-hidden="true"></i>
                      </span>
                    </span>{% endif %}
                    <div class="tt_sub_menu_wrap {% if block.settings.tt_mn_show_sub == false %}disable{% endif %}{% if block.settings.bg_img_collection != blank %} bg_img{% endif %}" {% if block.settings.tt_mn_sub_contentwidth != blank %}style="width:{{ block.settings.tt_mn_sub_contentwidth }}"{% endif %}>
                      {% endif %}
                      {%- assign ik = 0 -%}
                      {% endif %}
                      <style> 
                        #tt-megamenu #menu--{{ block.id }}.tt_menu_item .tt_sub_menu_wrap.bg_img{padding: 20px 15px 30px 15px; background-attachment:scroll; background-repeat:no-repeat; background-position:bottom; background-image:url('{{ block.settings.bg_img_collection | img_url: "master" }}');}
                          @media(max-width:991px){#tt-megamenu #menu--{{ block.id }}.tt_menu_item .tt_sub_menu_wrap.bg_img{background-image:none;}}
                      </style>

                      {% if block.settings.tt_mn_show_sub == true %}

                      {% if block.settings.tt_sub_menu_clinklist != blank or block.settings.tt_sub_menu_csecondlinklist != blank %}
                      <div class="tt_sub_menu" {% if block.settings.tt_sub_menu_cwidth != blank %}style="width:{{ block.settings.tt_sub_menu_cwidth }}"{% endif %}>
                        {% if block.settings.tt_sub_menu_ctitle != blank %}
                        <h4 class="sub_mn_title">
                          <a href="{{ block.settings.tt_sub_menu_clink }}">{{ block.settings.tt_sub_menu_ctitle }}</a>
                        </h4>
                        {% endif %}
                        {%- assign tt_sub_menu_c = block.settings.tt_sub_menu_clinklist -%}
                        {%- if linklists[tt_sub_menu_c].links != blank -%}
                        <ul class="tt_sub_menu_linklist">
                          {%- for link in linklists[tt_sub_menu_c].links -%}
                          <li{% if link.links != blank %} class="tt_menu_item"{% endif %}>
                            <a href="{{ link.url }}" title="{{ link.object.description | strip_html }}">{{ link.title }}</a>
                            {% if link.links != blank %}
                            <div class="tt_sub_menu_wrap">
                              <ul class="sub_menu_linklist">
                                {% for link in link.links %}
                                <li>
                                  <a href="{{ link.url }}" title="{{ link.object.description | strip_html }}">{{ link.title }}</a>
                                </li>
                                {% endfor %}
                              </ul>
                            </div>
                            {% endif %}
                          </li>
                          {% endfor %}
                        </ul>
                        {% endif %}

                        {% if block.settings.tt_sub_menu_csecondtitle != blank %}
                        <h4 class="sub_mn_title">
                          <a href="{{ block.settings.tt_sub_menu_csecondlink }}">{{ block.settings.tt_sub_menu_csecondtitle }}</a>
                        </h4>
                        {% endif %}    

                        {%- assign tt_sub_menu_csecondmenu = block.settings.tt_sub_menu_csecondlinklist -%}

                        {%- if linklists[tt_sub_menu_csecondmenu].links != blank -%}
                        <ul class="tt_sub_menu_linklist">
                          {%- for link in linklists[tt_sub_menu_csecondmenu].links -%}
                          <li>
                            <a href="{{ link.url }}" title="{{ link.object.description | strip_html }}">{{ link.title }}</a>
                            {% if link[tt_sub_menu_csecondmenu].links != blank %}
                            <ul class="tt_sub_menu_linklist">
                              {%- for sub_link in link[tt_sub_menu_csecondmenu].links -%}
                              <li>
                                <a href="{{ sub_link.url }}" title="{{ sub_link.object.description | strip_html }}">{{ sub_link.title }}</a>
                              </li>
                              {% endfor %}
                            </ul>
                            {% endif %}
                          </li>
                          {% endfor %}
                        </ul>
                        {% endif %}
                      </div>
                      {% endif %}
                      {% if block.settings.tt_sub_menu_cthirdlinklist != blank or block.settings.tt_sub_menu_cfrouthlinklist != blank %}
                      <div class="tt_sub_menu" {% if block.settings.tt_sub_menu_cwidth != blank %}style="width:{{ block.settings.tt_sub_menu_cwidth }}"{% endif %}>
                        {% if block.settings.tt_sub_menu_cthirdtitle != blank %}
                        <h4 class="sub_mn_title">
                          <a href="{{ block.settings.tt_sub_menu_cthirdlink }}">{{ block.settings.tt_sub_menu_cthirdtitle }}</a>
                        </h4>
                        {% endif %}
                        {%- assign tt_sub_menu_cthirdmenu = block.settings.tt_sub_menu_cthirdlinklist -%}

                        {%- if linklists[tt_sub_menu_cthirdmenu].links != blank -%}
                        <ul class="tt_sub_menu_linklist">
                          {%- for link in linklists[tt_sub_menu_cthirdmenu].links -%}
                          <li>
                            <a href="{{ link.url }}" title="{{ link.object.description | strip_html }}">{{ link.title }}</a>
                            {% if link[tt_sub_menu_cthirdmenu].links != blank %}
                            <ul class="tt_sub_menu_linklist">
                              {%- for sub_link in link[tt_sub_menu_cthirdmenu].links -%}
                              <li>
                                <a href="{{ sub_link.url }}" title="{{ sub_link.object.description | strip_html }}">{{ sub_link.title }}</a>
                              </li>
                              {% endfor %}
                            </ul>
                            {% endif %}
                          </li>
                          {% endfor %}
